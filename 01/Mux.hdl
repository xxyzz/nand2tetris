/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 * a's + bs
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not (in=sel, out=notsel);
    And (a=a, b=notsel, out=andout1);
    And (a=b, b=sel, out=andout2);
    Or (a=andout1, b=andout2, out=out);
}